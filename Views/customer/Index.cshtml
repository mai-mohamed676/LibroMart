@Model List<customer>
    @{
        ViewData["title"] = "index";
    }
<h1>index</h1>
<a href="/customer/new">new customer</a>
<table class="table table_borderd">
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>salary</th>
    </tr>
@foreach(customer item in Model){
    <tr>
            <td>@item.Name</td>
           <td>@item.salary</td>
          
     
           <td><a href="/customer/Details/@item.Id">Details</a></td>
    

   </tr>
}

</table>
